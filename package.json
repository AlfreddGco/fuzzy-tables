{
  "name": "fuzzy-tables",
  "version": "1.5.1",
  "description": "A modern React component library for fuzzy tables",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./type-utils": {
      "types": "./dist/type-utils.d.ts",
      "import": "./dist/type-utils.mjs",
      "require": "./dist/type-utils.js"
    },
    "./styles.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "vite demo",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "pre-push": "pnpm test:run && pnpm build"
  },
  "keywords": [
    "react",
    "table",
    "component",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.1",
    "clsx": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.456.0",
    "react-error-boundary": "^4.1.2",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.49",
    "postcss-modules": "^6.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.83.0",
    "tailwindcss": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^3.2.4"
  }
}
